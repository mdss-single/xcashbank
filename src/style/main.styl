/*  <footer>  */
.footer
	color #fff
	background-image linear-gradient(to right, #2472ef, #00a2cd)
	padding 34px 0 20px
	@media $desktop
		padding 0
		.container
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			align-items center
			height 72px
	&__phone
		font-weight 700
		font-size 1.8rem
		margin-bottom 10px
		@media $desktop
			display none
	&__worktime
		font-size 1.2rem
		margin-bottom 30px
		@media $desktop
			display none
	&__copyright
		font-size 1.4rem
	&__nav
		display none
		@media $desktop
			display block
			&-item
				margin-left 26px
/*  </footer>  */

/*  <menu>  */
.menu
	position fixed
	top 0
	right 0
	bottom 0
	overflow-y auto
	width 310px
	z-index 108
	background-color #fff
	padding 85px 15px 0 25px
	transform translateX(100%)
	transition all .5s
	@media $desktop
		width 42%
		padding 138px 0 0 45px
	&--active
		transform translateX(0)
	&__cover
		position fixed
		top 0
		right 0
		bottom 0
		left 0
		z-index 107
		background-color rgba(#151e29, .85)
	&__close
		position absolute
		top 38px
		left 25px
		width 28px
		height @width
		@media $desktop
			top 62px

/*  <vars>  */
$desktop = "screen and (min-width:1200px)";
$phone-land = "screen and (min-width:768px)";
$phone = "screen and (min-width:480px)";
/*  </vars>  */

/*  <fonts>  */
@font-face
	font-family 'G'
	src: url('../fonts/GothamPro-Light.eot?#iefix') format('embedded-opentype'), url('../fonts/GothamPro-Light.woff') format('woff'), url('../fonts/GothamPro-Light.ttf') format('truetype')
	font-weight 300
	font-style normal
@font-face
	font-family 'G'
	src url('../fonts/GothamPro.eot?#iefix') format('embedded-opentype'), url('../fonts/GothamPro.woff') format('woff'), url('../fonts/GothamPro.ttf') format('truetype')
	font-weight normal
	font-style normal
@font-face
	font-family 'G'
	src url('../fonts/GothamPro-Medium.eot?#iefix') format('embedded-opentype'), url('../fonts/GothamPro-Medium.woff') format('woff'), url('../fonts/GothamPro-Medium.ttf') format('truetype')
	font-weight 500
	font-style normal
@font-face
	font-family 'G'
	src url('../fonts/GothamPro-Bold.eot?#iefix') format('embedded-opentype'), url('../fonts/GothamPro-Bold.woff') format('woff'), url('../fonts/GothamPro-Bold.ttf') format('truetype')
	font-weight 700
	font-style normal
/*  </fonts>  */

/*  <general>  */
body
	display -webkit-flex
	display -moz-flex
	display -ms-flex
	display -o-flex
	display flex
	flex-direction column
	color #000
	font normal 1.4rem/1.3 G
	background-color #fff
:-moz-placeholder
	opacity 1
	color currentColor
:-ms-input-placeholder
	opacity: 1
	color currentColor
a
	color inherit
	font inherit
	text-decoration none
	transition all .2s
	&:hover
		text-decoration underline
.main
	flex 1
/*  </general>  */

/*  <header>  */
.header
	flex-shrink 0
	height 108px
	&:before
		position absolute
		top 0
		right 0
		left 0
		z-index -1
		height 56px
		background-image linear-gradient(to right, #2472ef, #00a2cd)
		content ''
	&__burger
		position absolute
		top 19px
		left 15px
		width 20px
		height 14px
		border solid #fff
		border-width 2px 0
		@media $desktop
			display none
		&:before
			position absolute
			top 50%
			right 0
			left 0
			height 2px
			background-color #fff
			content ''
			transform translateY(-50%)
	&__logo
		position absolute
		top 14px
		left 50%
		color #fff
		font-weight 700
		font-size 2.1rem
		text-transform uppercase
		transform translateX(-50%)
		@media $desktop
			left 15px
			transform none
		&:hover
			text-decoration none
		& img
			vertical-align -3px
			margin-right 5px
	&__contacts
		display none
		@media $desktop
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content flex-start
			position absolute
			top 0
			left 240px
			color #fff
			font-weight 700
			line-height 52px
			&-item
				padding-bottom 4px// (blue height = 56px) - (line-height = 52px)
				margin-right 25px
				&:last-child
					margin-right 0
	&__phone
		display none
		@media $desktop
			display block
			position absolute
			top 8px
			right 110px
			color #fff
			text-align right
			&-number
				font-weight 700
				font-size 1.8rem
			&-worktime
				font-size 1.2rem
				margin 0
	&__login
		position absolute
		top 0
		right 5px
		width 42px
		height 56px
		background url('../i/icons/icon-auth.svg') no-repeat center 15px / 22px auto
		@media $desktop
			right 15px
			width 68px
			border solid rgba(#fff, .39)
			border-width 0 1px
			&:before
				transition all .5s
			&:hover
				&:before
					animation fadeIn .5s
					position absolute
					top 0
					right 0
					bottom 0
					left 0
					z-index -1
					background-image linear-gradient(rgba(#2472ef, .23), rgba(#51d9fe, .23))
					content ''
	&__rates
		position relative
		top 56px
		overflow-x auto
		height 52px
		line-height 52px
		white-space nowrap
		@media $desktop
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			align-items center
			overflow-x initial
			line-height 1
			white-space pre-wrap
			padding-right 68px
		.flickity-viewport
			mask-image:linear-gradient(to right, rgba(255, 255, 255, 1) 60%, rgba(0, 0, 255, .2));
		&-item
			font-weight 500
			font-size 1.2rem
			margin-right 32px
			@media $desktop
				&:last-of-type
					margin-right 0
		&-currency
			color #0ec0ff
		&-next
			position absolute
			top 13px
			right 0
			z-index 3
			width 26px
			height @width
			background-color transparent
			background-image url('data:image/svg+xml;base64,')
			border 0
			@media $desktop
				display none
		&-info
			display none
			@media $desktop
				display block
				position absolute
				top 0
				bottom 0
				right 21px
				width 26px
				background url('../i/icons/icon-info.png') no-repeat center center
	&__menu
		display none
		position absolute
		top 56px
		right 0
		left 0
		z-index 100
		color #fff
		background-color #2274ee
		padding 0 15px
		box-shadow 18px 15px 27px rgba(#000, .44)
		&--active
			display block
			animation fadeIn .5s
	&-menu
		&__close
			position absolute
			top -56px
			left 0
			width 62px
			height 56px
			background #2274ee url('../i/header-menu__close.png') no-repeat 17px center
		&__contacts
			margin 25px 0 20px
			&-item
				display block
				font-weight 700
				font-size 1.4rem
				margin 12px 0
		&__phone
			margin 20px 0 32px
			&-number
				font-weight 700
				font-size 1.8rem
			&-worktime
				font-size 1.2rem
				margin 0
		&__nav
			margin 30px 0
			&-item
				display block
				font-weight 700
				font-size 1.8rem
				margin 15px 0
		&__user
			margin 25px 0
			&-item
				display block
				font-size 1.8rem
				padding 5px 0
				margin 10px 0
				&--active
					font-weight 700
/*  </header>  */

/*  <exchange>  */
.exchange
	background-color #edf5fc
	@media $desktop
		padding-bottom 42px
		&__wrap
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items flex-start
			align-items stretch
			position relative
			border-radius 0 0 10px 10px
			padding-top 87px
			margin 0 -47px 0 -50px
			&:before
				position absolute
				top 87px
				right 0
				bottom 0
				left 0
				border-radius 0 0 10px 10px
				box-shadow 0 17px 9px rgba(#4a6a81, .1)
				content ''
	&__col
		@media $desktop
			position relative
			background-color #fff
		&--give
			padding 20px 0 45px
			@media $desktop
				width 349px
				border-radius 0 0 0 10px
				padding 0
		&--get
			@media $desktop
				flex 1
				border solid #e6e6e6
				border-width 0 1px
		&--info
			background-color #fff
			padding 15px 15px 0
			margin 28px -15px 0
			@media $desktop
				width 410px
				border-radius 0 0 10px
				padding 20px 40px
				margin 0
	&-col
		&__header
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			align-items center
			margin-bottom 16px
			@media $desktop
				justify-content flex-start
				position absolute
				top -67px
				right 0
				left 0
				height 67px
				^[0]__col--give &
					background-color #f4f9fe
					border-radius 10px 0 0
					padding-left 50px
				^[0]__col--get &
					background-color #f4f9fe
					border-radius 0 10px 0 0
					border-left 1px solid #e6e6e6
					padding 0 28px 0 22px
					margin-left -1px
				^[0]__col--info &
					padding-left 40px
		&__summ
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between;
			align-items center
			position relative
			height 58px
			background-color #fff
			border 1px solid #0dc3ff
			border-radius 3px
			padding 0 8px 0 10px
			@media $desktop
				padding 0 8px 0 15px
				margin-bottom 22px
				^[0]__col--give &
					width 314px
					margin-right -1px
					margin-left auto
				^[0]__col--get &
					width 322px
					margin-left -1px
			&:before
				position absolute
				top 50%
				right 46px
				z-index 1
				color #b2b2b2
				font-size 1.1rem
				content attr(data-text)
				transform translateY(-50%)
				@media $desktop
					right 84px
		&__mobile
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			position relative
			height 58px
			background-color #fff
			border 1px solid #0dc3ff
			border-radius 3px
			padding 0 8px 0 15px
			margin-top 11px
			@media $desktop
				display none
			&:before
				position absolute
				top 50%
				right 16px
				width 13px
				height 7px
				background-image url('data:image/svg+xml;base64,')
				content ''
				transform translateY(-50%)
			&-icon
				flex-shrink 0
				max-height 35px
			&-name
				flex 1
				font-size 1.6rem
				margin-left 12px
		&__currency
			display none
			@media $desktop
				display block
				margin-bottom 25px
	&-header
		&__title
			font-weight 500
			font-size 1.6rem
			@media $desktop
				margin-top -3px
				margin-right 20px
		&__currency
			@media $desktop
				flex 1
			&-item
				position absolute
				left -9999px
				& + label
					color #9f9f9f
					font-weight 500
					font-size 1.2rem
					line-height 2.6rem
					border-radius 13px
					padding 6px 10px
					cursor pointer
					transition all .2s
				&:checked + label
					color #fff
					background-color #0dc3ff
					cursor default
		&__reserv
			display none
			@media $desktop
				display block
		&__icons
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			align-items center
			&-item
				&:last-child
					background url('data:image/svg+xml;base64,') no-repeat 18px center / 7px auto
					padding-left 40px
				img
					max-height 35px
	&-summ
		&__value
			flex 1
			z-index 2
			height 56px
			font-weight 500
			font-size 1.6rem
			background-color transparent
			border 0
			span&
				line-height 56px
		&__request
			color #0dc3ff
			font-size 1.2rem
			margin-right 17px
		&__icon
			flex-shrink 0
			max-height 35px
		&__currency
			display none
			@media $desktop
				display block
				font-weight 500
				font-size 1.2rem
				margin-left 10px
	&-wallet
		margin-top 25px
		&__empty
			animation backgroundLoad 3s infinite
			position relative
			height 30px
		&__title
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			align-items center
			font-size 1.6rem
			margin-bottom 8px
			&-descr
				color #b2b2b2
				font-size 1.4rem
		&__input
			position relative
		&__icon
			position absolute
			top 50%
			right 14px
			transform translateY(-50%)
	&-info
		&__summ
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			background url('../i/exchange-total.png') no-repeat center 16px
			&-title
				color #a1a1a1
				font-size 1.2rem
				margin 0
			&-value
				font-weight 500
				font-size 3rem
				margin 0
			&-currency
				display none
		&__detail
			font-size 1.2rem
			@media $desktop
				position relative
			& > span:nth-child(2)
				display block
				@media $desktop
					display inline
		&__btn
			.btn
				display block
				width 100%
				@media $desktop
					width auto
					padding 0 50px
					margin 40px auto 0
	&-currency
		&__item
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			align-items center
			position relative
			border-bottom 1px solid #e6e6e6
			padding 4px 20px 4px 50px
			cursor pointer
			transition all .2s
			@media $desktop
				^[0]__col--give &
					padding 4px 10px 4px 50px
				^[0]__col--get &
					padding 4px 30px 4px 18px
				&:last-child
					border-bottom 0
			^[0]__col--give &
				&:hover
					& ^[0]-currency__item
						&-icon
						&-value
						&-name
							position relative
							color #fff
					&:before
						animation fadeIn .2s
						position absolute
						top 0
						right 0
						bottom 0
						left 0
						z-index 0
						background-image linear-gradient(to right, #352b67, #5a4ca0)
						content ''
			^[0]__col--get &
				&:hover
					background-color #f2f0f0
			&--active
				color #fff
				&:before
					animation fadeIn .2s
					position absolute
					top 0
					right 0
					bottom 0
					left 0
					z-index 0
					background-image linear-gradient(to right, #352b67, #5a4ca0)
					content ''
				.menu-exchange &
					position relative
					&:before
						position absolute
						top 50%
						right 10px
						width 20px
						height @width
						background-image url('../i/icons/icon-ok.svg')
						content ''
						transform translateY(-50%)
				& ^[-1]-icon
					position relative
					overflow hidden
					background-color #fff
					border-radius 50%
				& ^[-1]-value
				& ^[-1]-name
				& ^[-1]-reserv
					position relative
					color #fff
			.menu-exchange &
				padding 4px 32px 4px 25px
				margin 0 -15px 0 -25px
			&-icon
				flex 0 0 35px
				display -webkit-flex
				display -moz-flex
				display -ms-flex
				display -o-flex
				display flex
				justify-content center
				align-items center
				width 35px
				height 35px
				text-align center
				margin-right 15px
			&-value
				min-width 45px
				color #999
				^[0]__col--get &
					min-width 80px
			&-name
				flex 1
			@media $desktop
				&-reserv
					color #999
/*  </exchange>  */

/*  <homepage-about-us>  */
.homepage-about-us
	padding 50px 0 45px
	@media $desktop
		padding 40px 0
		&__info
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			justify-content space-between
			margin-top 50px
	&__title
		font-weight 500
		font-size 2.4rem
		margin 0
	&__item
		margin-top 25px
		@media $desktop
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			margin-top 0
			&:first-child
				& ^[1]-title
					width 115px
			&:last-child
				& ^[1]-title
					width 80px
		&-title
			display block
			font-size 1.2rem
			@media $desktop
				width 70px
		&-value
			font-weight 500
			font-size 3.3rem
		&-currency
			font-weight 700
			font-size 1.4rem
/*  </homepage-about-us>  */

/*  <homepage-latest>  */
.homepage-latest
	background-color #eaf3fb
	padding 30px 0
	@media $desktop
		padding 82px 0 32px
		.container
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
.latest-actions
	@media $desktop
		flex 1
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		flex-wrap wrap
		justify-content space-between
		position relative
		margin-right 30px
	&__title
		font-weight 500
		font-size 2.4rem
		margin 0 0 20px
		@media $desktop
			position absolute
			top -50px
			left 0
	&__item
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		flex-wrap wrap
		justify-content space-between
		background-color #fff
		border-radius 3px
		padding 22px 25px
		margin 28px 0
		box-shadow 0 10px 21px rgba(#4a6a81, .1)
		@media $desktop
			width calc(50% - 15px)
			padding 28px 50px 20px
			margin 0 0 28px
		&-date
			width 100%
			font-size 1.2rem
			margin 0 0 20px
		&-info
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			align-items center
			img
				flex-shrink 0
				margin-right 8px
		&-value
			font-weight 700
			font-size 1.4rem
			margin 0
			span
				display block
		&-delimeter
			width 38px
			height 38px
			background-image url('../i/exchange-total.png')
.latest-reviews
	position relative
	background-color #fff
	border-radius 3px
	padding 20px 20px 30px 30px
	margin-top 82px
	box-shadow 0 10px 21px rgba(#4a6a81, .1)
	@media $desktop
		width 360px
		margin-top 0
		margin-bottom 28px
	&__title
		position absolute
		top -50px
		left 0
		font-weight 500
		font-size 2.4rem
		margin 0
		@media $desktop
			position absolute
			top -45px
			left 0
	&__list
		margin-bottom 70px
		.flickity-page-dots
			bottom -35px
			text-align left
			.dot
				opacity 1
				background #000
				margin 0 5px 0 0
				&.is-selected
					background-color #0dc3ff
	&__item
		width 100%
	&__info
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		justify-content space-between
		align-items center
		margin-bottom 15px
	&__author
		font-weight 700
		font-size 1.6rem
		margin 0
	&__date
		font-size 1.2rem
	&__text
		line-height 2.4rem
	&__add
		color #0cc5ff
		font-weight 700
		font-size 1.4rem
/*  </homepage-latest>  */

